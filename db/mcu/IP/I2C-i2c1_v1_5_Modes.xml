<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<IP xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://mcd.rou.st.com/modules.php?name=mcu" 
    DBVersion="V3.0" 
    Name="I2C" 
    ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../db/IP_Modes_V3_0.xsd" 
    Version="i2c1_v1_5" 
    IPType="peripheral">
    
    <ModeLogicOperator Name="OR">
        <Mode Name="I2C">
            <ModeLogicOperator Name="XOR">
                <Mode Name="I2C">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="SCL"/>
                        <Signal Name="SDA"/>
                    </SignalLogicalOp>
                </Mode>
		        <Mode Name="SMBus-Alert-mode">
					<SignalLogicalOp Name="AND">
						<Signal Name="SCL"/>
						<Signal Name="SDA"/>
						<Signal Name="SMBA"/>
					</SignalLogicalOp>
				</Mode>
				<Mode Name="SMBus-two-wire-Interface">
					<SignalLogicalOp Name="AND">
						<Signal Name="SCL"/>
						<Signal Name="SDA"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
    </ModeLogicOperator>
    <RefSignal IOMode="GPIO" Name="SCL"/>        
    <RefSignal IOMode="GPIO" Name="SDA"/>       
    <RefSignal IOMode="GPIO" Name="SMBA"/>
</IP>
