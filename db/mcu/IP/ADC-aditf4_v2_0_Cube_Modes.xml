        <PossibleValue Comment="Asynchronous clock mode divided by 2" Value="ADC_CLOCK_ASYNC_DIV2" Semaphore="ADC_RCC2"/>
        <PossibleValue Comment="Asynchronous clock mode divided by 4" Value="ADC_CLOCK_ASYNC_DIV4" Semaphore="ADC_RCC3"/>
