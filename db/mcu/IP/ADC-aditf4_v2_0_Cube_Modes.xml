    </RefParameter>
    <RefParameter  Comment="Clock Prescaler" DefaultValue="false" Name="EnableClockSemaphore" Type="boolean" Group="ADCgroup" Visible="false">
        <PossibleValue Comment="RCC" Value="false"/>
    </RefParameter>
		
