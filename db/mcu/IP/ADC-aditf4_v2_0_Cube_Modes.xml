    <RefParameter Comment="Resolution" DefaultValue="ADC_RESOLUTION12b" Name="Resolution" Type="list" Group="ADCgroup">
        <PossibleValue Comment="ADC 12-bit resolution" Value="ADC_RESOLUTION12b" Semaphore="ADC_RESOLUTION12b"/>
        <PossibleValue Comment="ADC 10-bit resolution" Value="ADC_RESOLUTION10b" Semaphore="ADC_RESOLUTION10b"/>
