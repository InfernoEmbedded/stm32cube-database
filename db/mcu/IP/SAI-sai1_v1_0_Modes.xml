						<Signal Name="MCLK_B" />
					</SignalLogicalOp>
					<Semaphore>$IpInstance_SAIB_Master</Semaphore>
				</Mode>
				<Mode Name="SAI_B_AsyncSlave" UserName="Asynchronous Slave">
