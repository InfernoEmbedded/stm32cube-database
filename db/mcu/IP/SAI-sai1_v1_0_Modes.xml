						<Signal Name="SD_B" />
						<Signal Name="SCK_B" />
						<Signal Name="FS_B" />
					</SignalLogicalOp>
					<Semaphore>$IpInstance_SAIB_AsyncSlave</Semaphore>
				</Mode>
				<Mode Name="SAI_B_SyncSlave" UserName="Synchronous Slave" ShrinkName="SAI_B in Synchronous Slave Mode" 
					RemoveCondition="(!$ModeExist_SAI_A_Master) &amp; (!$ModeExist_SAI_A_MasterWithClock) &amp; (!$ModeExist_SAI_A_AsyncSlave)" >
					<SignalLogicalOp Name="AND">
