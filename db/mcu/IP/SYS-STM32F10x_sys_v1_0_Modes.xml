					<SignalLogicalOp Name="AND">
						<Signal Name="TAMPER-RTC"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
		</Mode>
                <Mode Name="MISC_REMAP" UserName="Miscellaneous features remapping (DAC and NVIC)" RemoveCondition="!DIE428">
